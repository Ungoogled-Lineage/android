<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    <!-- Get rid of Lineage repos -->
    <remove-project name="LineageOS/android_frameworks_base" />
    <remove-project name="LineageOS/android_frameworks_native" />
    <remove-project name="LineageOS/android_packages_apps_Contacts" />
    <remove-project name="LineageOS/android_packages_apps_ContactsCommon" />
    <remove-project name="LineageOS/android_packages_apps_ExactCalculator" />
    <remove-project name="LineageOS/android_packages_apps_PackageInstaller" />
    <remove-project name="LineageOS/android_packages_apps_PhoneCommon" />
    <remove-project name="LineageOS/android_packages_apps_Settings" />
    <remove-project name="LineageOS/android_system_sepolicy" />
    <remove-project name="LineageOS/android_vendor_cm" />
    <remove-project name="LineageOS/android_build" />
    <remove-project name="LineageOS/android_packages_apps_Email" />

    <!-- Track our repos -->
    <project path="frameworks/base" name="Ungoogled-Lineage/android_frameworks_base" />
    <project path="frameworks/native" name="Ungoogled-Lineage/android_frameworks_native" />
    <project path="packages/apps/Contacts" name="Ungoogled-Lineage/android_packages_apps_Contacts" />
    <project path="packages/apps/ContactsCommon" name="Ungoogled-Lineage/android_packages_apps_ContactsCommon" />
    <project path="packages/apps/ExactCalculator" name="Ungoogled-Lineage/android_packages_apps_ExactCalculator" />
    <project path="packages/apps/PackageInstaller" name="Ungoogled-Lineage/android_packages_apps_PackageInstaller" />
    <project path="packages/apps/PhoneCommon" name="Ungoogled-Lineage/android_packages_apps_PhoneCommon" />
    <project path="packages/apps/Settings" name="Ungoogled-Lineage/android_packages_apps_Settings" />
    <project path="system/sepolicy" name="Ungoogled-Lineage/android_system_sepolicy" />
    <project path="vendor/cm" name="Ungoogled-Lineage/android_vendor_ungoogled" />
    <project path="packages/apps/Dialer" name="Ungoogled-Lineage/android_packages_apps_Dialer" groups="pdk-fs" />
    <project path="packages/apps/SetupWizard" name="Ungoogled-Lineage/android_packages_apps_SetupWizard" />
    <project path="build" name="Ungoogled-Lineage/android_build" groups="pdk"/>
    <project path="packages/apps/Email" name="Ungoogled-Lineage/android_packages_apps_Email" groups="pdk-fs" />
    <project path="packages/apps/MusicFX" name="Ungoogled-Lineage/android_packages_apps_MusicFX" groups="pdk-fs" />

    <!-- Track ThemeInterfacer -->
    <project path="packages/services/ThemeInterfacer" name="substratum/interfacer" remote="github" revision="n-rootless" />
</manifest>
